class Solution { //S.C -> O(n)
    public int firstMissingPositive(int[] nums) {
        
        Set<Integer> st = new HashSet<>();
        
        for(int i = 0; i<nums.length; i++){
            
            st.add(nums[i]);
            
        }
        
        for(int i = 1; i<=(nums.length+1); i++){
            
            if(!st.contains(i)){
                return i;
            }
        }
        return 1;
                
    }
}
