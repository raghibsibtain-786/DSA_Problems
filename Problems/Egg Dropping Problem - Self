public int solveEggDrop(int f, int e)
{
        if(f==0 || f==1)
        {
            return f;
        }
        if(e==1)
        {
            return f;
        }
        
        int min = Integer.MAX_VALUE;
        
        for(int k = 1; k<=f; k++){
        
            int temp = 1 + Math.max(solveEggDrop(e-1,f-1),solveEggDrop(e,f-k));
            
            if(temp<min){
                min = temp;
            }
        }
        
        return min;
}
